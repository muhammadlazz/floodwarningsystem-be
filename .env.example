# Database
DATABASE_URL="postgresql://user:password@localhost:5432/floodwarning?schema=public"

# Server
PORT=3000
NODE_ENV=development

# JWT (REQUIRED - no default value for security)
JWT_SECRET=your-super-secret-key-here-min-32-chars
JWT_EXPIRE=7d

# CORS
CORS_ORIGIN=http://localhost:3000

# BBWS Sync Job
BBWS_SYNC_ENABLED=false
BBWS_SYNC_URL=https://your-bbws-api-url.com/api/water-levels
BBWS_SYNC_INTERVAL_MS=300000
BBWS_SYNC_TIMEOUT_MS=15000
